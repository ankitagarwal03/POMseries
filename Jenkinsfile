                echo("Project is getting build")
            }
        }

        stage("deploy to QA"){
            steps{
                echo("deploying to QA")
            }
        }

        stage("deploy to Dev"){
            steps{
                echo("deploying to DEV")
            }
        }

        stage("running Sanity"){
            steps{
                echo("running sanity on the QA")
            }
        }

        stage("running Sanity"){
             steps{
                  echo("running sanity on the QA")
             }
        }


    }
}